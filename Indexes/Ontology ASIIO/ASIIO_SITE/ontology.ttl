                            owl:onProperty :extAffests ;
                            owl:minQualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :extAsset
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :extHasCriticality ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :extCriticalityValuePartition
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://www.w3.org/2006/time#hasBeginning> ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass <http://www.w3.org/2006/time#Instant>
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://www.w3.org/2006/time#hasEnd> ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass <http://www.w3.org/2006/time#Instant>
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :extDescription ;
                            owl:someValuesFrom rdfs:Literal
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :extID ;
                            owl:someValuesFrom xsd:long
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :extType ;
                            owl:someValuesFrom rdfs:Literal
                          ] ;
          <http://purl.org/dc/terms/created> "14/Nov/2019" ;
