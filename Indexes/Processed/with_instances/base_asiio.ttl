###  http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extAlarm
:extAlarm rdf:type owl:Class ;
          owl:equivalentClass [ owl:intersectionOf ( :extAlert
                                                     [ rdf:type owl:Class ;
                                                       owl:unionOf ( [ rdf:type owl:Restriction ;
                                                                       owl:onProperty :extHasSeverity ;
                                                                       owl:someValuesFrom :extExtremeSeverity
                                                                     ]
                                                                     [ rdf:type owl:Restriction ;
                                                                       owl:onProperty :extHasSeverity ;
                                                                       owl:someValuesFrom :extHighSeverity
                                                                     ]
                                                                   )
                                                     ]
                                                   ) ;
                                rdf:type owl:Class
                              ] ;
          terms:created "14/Nov/2019" ;
          terms:creator "Katia Aleid" ;
          rdfs:label "Alarm"@en ,
                     "Alarme"@pt ;
          owl:versionInfo "1" ;
          skos:definition "An alert with a Security Level that is either High or Extreme" .


###  http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extAlert
:extAlert rdf:type owl:Class ;
          rdfs:subClassOf :extThing ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :extHasSeverity ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :extSeverityValuePartition
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :extHasStatus ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :extStatusValuePartition
                          ] ,
