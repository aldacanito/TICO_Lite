###  http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extEvent
:extEvent rdf:type owl:Class ;
          rdfs:subClassOf :extThing ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :extChanges ;
                            owl:someValuesFrom :extAsset
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :extTriggers ;
                            owl:someValuesFrom :extAlert
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :extHasCriticality ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass :extCriticalityValuePartition
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://www.w3.org/2006/time#hasBeginning> ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass <http://www.w3.org/2006/time#Instant>
                          ] ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty <http://www.w3.org/2006/time#hasEnd> ;
                            owl:qualifiedCardinality "1"^^xsd:nonNegativeInteger ;
                            owl:onClass <http://www.w3.org/2006/time#Instant>
                          ] ;
          terms:created "14/Nov/2019" ;
          terms:creator "Katia Aleid" ;
          terms:modified "30/Nov/2019" ;
          rdfs:label "Event"@en ,
                     "Evento"@pt ;
          owl:versionInfo 2 ;
          skos:definition "Discrete change of state or status of a system/device"@en .


