<http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension> rdf:type owl:Ontology ;
                                                                                        owl:imports <http://purl.org/cyber/uco> ,
                                                                                                    <http://www.w3.org/2006/time#2016> ,
                                                                                                    <https://data.nasa.gov/ontologies/atmonto/equipment> ;
                                                                                        rdfs:comment """This is an extension to UCO ontology that started with 4 main concepts 
Incidient
Event
Alert
Alarm"""^^xsd:string ;
                                                                                        owl:versionInfo 3 .

#################################################################
#    Annotation properties
#################################################################

###  http://purl.org/dc/terms/created
terms:created rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/creator
terms:creator rdf:type owl:AnnotationProperty .


###  http://purl.org/dc/terms/modified
terms:modified rdf:type owl:AnnotationProperty .


###  http://www.w3.org/2004/02/skos/core#definition
skos:definition rdf:type owl:AnnotationProperty .


#################################################################
#    Object Properties
#################################################################

###  http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extAffests
:extAffests rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:label "affects"@en .


###  http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extChanges
:extChanges rdf:type owl:ObjectProperty ;
            rdfs:subPropertyOf owl:topObjectProperty ;
            rdfs:comment "each event changes an asset" ;
            rdfs:label "changes" ;
            owl:versionInfo 1 .


###  http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extConsistsOf
:extConsistsOf rdf:type owl:ObjectProperty ,
                        owl:AsymmetricProperty ;
               rdfs:domain :extIncident ;
               rdfs:range :extEvent ;
               rdfs:comment "All incidents are events, but not all events are incidents so this property is asymmetric" ;
               rdfs:label "consistsOf" ;
               owl:versionInfo 1 .


###  http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extExploits
:extExploits rdf:type owl:ObjectProperty ;
             rdfs:subPropertyOf owl:topObjectProperty ;
             rdfs:label "exploits"@en .


###  http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extHas
:extHas rdf:type owl:ObjectProperty ;
        rdfs:label "has"@en .


###  http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extHasCriticality
:extHasCriticality rdf:type owl:ObjectProperty ,
                            owl:FunctionalProperty ;
                   rdfs:domain :extEvent ;
                   rdfs:range :extCriticalityValuePartition ;
                   rdfs:comment "each event has a level of criticality" ;
                   rdfs:label "hasCriticality" ;
                   owl:versionInfo 1 .


###  http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extHasSeverity
:extHasSeverity rdf:type owl:ObjectProperty ,
                         owl:FunctionalProperty ;
                rdfs:domain :extAlert ;
                rdfs:range :extSeverityValuePartition ;
                rdfs:label "hasSeverity" ;
                owl:versionInfo 1 .


###  http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extHasStatus
:extHasStatus rdf:type owl:ObjectProperty ,
                       owl:FunctionalProperty ;
              rdfs:domain :extAlert ;
              rdfs:range :extStatusValuePartition ;
              rdfs:label "hasStatus" ;
              owl:versionInfo 1 .


###  http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extIsTriggeredBy
:extIsTriggeredBy rdf:type owl:ObjectProperty ;
                  owl:inverseOf :extTriggers ;
                  rdf:type owl:FunctionalProperty ;
                  rdfs:domain :extAlert ;
                  rdfs:range :extEvent ;
                  rdfs:label "triggeredBy" ;
                  owl:versionInfo 1 .


###  http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extTriggers
:extTriggers rdf:type owl:ObjectProperty ;
             rdfs:domain :extEvent ;
             rdfs:range :extAlert ;
             rdfs:label "triggers" ;
             owl:versionInfo 1 .


###  http://www.w3.org/2006/time#hasBeginning
<http://www.w3.org/2006/time#hasBeginning> rdf:type owl:ObjectProperty .


###  http://www.w3.org/2006/time#hasEnd
<http://www.w3.org/2006/time#hasEnd> rdf:type owl:ObjectProperty .


