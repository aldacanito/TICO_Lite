                               owl:onClass <http://www.w3.org/2006/time#Instant>
                             ] ;
             terms:created "14/Nov/2019" ;
             terms:creator "Katia Aleid" ;
             terms:modified "15/Nov/2019" ;
             rdfs:label "Incident"@en ,
                        "Incidente"@pt ;
             owl:versionInfo 1 ;
             skos:definition "Event (or group of events) attributable to a human root cause"@en .


