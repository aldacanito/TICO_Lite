<?xml version="1.0"?>
<rdf:RDF xmlns="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#"
     xml:base="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension"
     xmlns:dc="http://purl.org/dc/elements/1.1/"
     xmlns:ext="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension"
     xmlns:owl="http://www.w3.org/2002/07/owl#"
     xmlns:rdf="http://www.w3.org/1999/02/22-rdf-syntax-ns#"
     xmlns:xml="http://www.w3.org/XML/1998/namespace"
     xmlns:xsd="http://www.w3.org/2001/XMLSchema#"
     xmlns:rdfs="http://www.w3.org/2000/01/rdf-schema#"
     xmlns:skos="http://www.w3.org/2004/02/skos/core#"
     xmlns:terms="http://purl.org/dc/terms/">
    <owl:Ontology rdf:about="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension">
        <owl:imports rdf:resource="http://purl.org/cyber/uco"/>
        <owl:imports rdf:resource="http://www.semanticweb.org/ontologies/2008/11/OntologySecurity.owl"/>
        <owl:imports rdf:resource="http://www.w3.org/2006/time#2016"/>
        <rdfs:comment>This is an extension to UCO ontology that contains 4 main concepts 
Incidient
Event
Alert
Alarm</rdfs:comment>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</owl:versionInfo>
    </owl:Ontology>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Object Properties
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extChanges -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extChanges">
        <rdfs:subPropertyOf rdf:resource="http://www.w3.org/2002/07/owl#topObjectProperty"/>
        <rdfs:comment>each event changes an asset</rdfs:comment>
        <rdfs:label>changes</rdfs:label>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extConsistsOf -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extConsistsOf">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AsymmetricProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extIncident"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extEvent"/>
        <rdfs:comment>All incidents are events, but not all events are incidents so this property is asymmetric</rdfs:comment>
        <rdfs:label>consistsOf</rdfs:label>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extHasCriticality -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extHasCriticality">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extEvent"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extCriticalityValuePartition"/>
        <rdfs:comment>each event has a level of criticality</rdfs:comment>
        <rdfs:label>hasCriticality</rdfs:label>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extHasSeverity -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extHasSeverity">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extAlert"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extSeverityValuePartition"/>
        <rdfs:label>hasSeverity</rdfs:label>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extHasStatus -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extHasStatus">
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extAlert"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extStatusValuePartition"/>
        <rdfs:label>hasStatus</rdfs:label>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extIsTriggeredBy -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extIsTriggeredBy">
        <owl:inverseOf rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extTriggers"/>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#FunctionalProperty"/>
        <rdfs:domain rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extAlert"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extEvent"/>
        <rdfs:label>triggeredBy</rdfs:label>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extTriggers -->

    <owl:ObjectProperty rdf:about="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extTriggers">
        <rdfs:domain rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extEvent"/>
        <rdfs:range rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extAlert"/>
        <rdfs:label>triggers</rdfs:label>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:versionInfo>
    </owl:ObjectProperty>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Classes
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    


    <!-- http://purl.org/cyber/ucoIncident -->

    <rdf:Description rdf:about="http://purl.org/cyber/ucoIncident">
        <owl:equivalentClass rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extIncident"/>
    </rdf:Description>
    


    <!-- http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extAlarm -->

    <owl:Class rdf:about="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extAlarm">
        <owl:equivalentClass>
            <owl:Class>
                <owl:intersectionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extAlert"/>
                    <owl:Class>
                        <owl:unionOf rdf:parseType="Collection">
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extHasSeverity"/>
                                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extExtremeSeverity"/>
                            </owl:Restriction>
                            <owl:Restriction>
                                <owl:onProperty rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extHasSeverity"/>
                                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extHighSeverity"/>
                            </owl:Restriction>
                        </owl:unionOf>
                    </owl:Class>
                </owl:intersectionOf>
            </owl:Class>
        </owl:equivalentClass>
        <terms:created>14/Nov/2019</terms:created>
        <terms:creator>Katia Aleid</terms:creator>
        <rdfs:label xml:lang="en">Alarm</rdfs:label>
        <rdfs:label xml:lang="pt">Alarme</rdfs:label>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:versionInfo>
        <skos:definition>An alert with a Security Level that is either High or Extreme</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extAlert -->

    <owl:Class rdf:about="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extAlert">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extHasSeverity"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extSeverityValuePartition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extHasStatus"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extStatusValuePartition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extIsTriggeredBy"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extEvent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <terms:created>14/Nov/2019</terms:created>
        <terms:creator>Katia Aleid</terms:creator>
        <rdfs:label xml:lang="en">Alert</rdfs:label>
        <rdfs:label xml:lang="pt">Alerta</rdfs:label>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:versionInfo>
        <skos:definition xml:lang="en">A notification that a specific attack has been directed at an organization’s information systems</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extAsset -->

    <owl:Class rdf:about="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extAsset">
        <owl:equivalentClass rdf:resource="http://www.semanticweb.org/ontologies/2008/11/OntologySecurity.owl#Asset"/>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extThing"/>
        <terms:created xml:lang="en">30/Nov/2019</terms:created>
        <terms:creator xml:lang="en">Katia Aleid</terms:creator>
        <rdfs:label xml:lang="en">Asset</rdfs:label>
        <owl:versionInfo xml:lang="en">1</owl:versionInfo>
        <skos:definition xml:lang="en">Information or resource which have value to an organization or person.</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extCriticalityValuePartition -->

    <owl:Class rdf:about="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extCriticalityValuePartition">
        <owl:equivalentClass>
            <owl:Class>
                <owl:unionOf rdf:parseType="Collection">
                    <rdf:Description rdf:about="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extEmergencyCriticality"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extEscalationCriticality"/>
                    <rdf:Description rdf:about="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extNormalCriticality"/>
                </owl:unionOf>
            </owl:Class>
        </owl:equivalentClass>
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extValuePartition"/>
        <terms:created>14/Nov/2019</terms:created>
        <terms:creator>Katia Aleid</terms:creator>
        <rdfs:comment>Available criticality levels are:
Normal
Escalation
Emergency</rdfs:comment>
        <rdfs:label xml:lang="en">Criticality</rdfs:label>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:versionInfo>
        <skos:definition xml:lang="en">Value partition that conrols the level of criticality of an event.</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extDisabledStaus -->

    <owl:Class rdf:about="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extDisabledStaus">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extStatusValuePartition"/>
        <owl:disjointWith rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extEnabledStatus"/>
        <terms:created>14/Nov/2019</terms:created>
        <terms:creator xml:lang="pt">Katia Aleid</terms:creator>
        <rdfs:label xml:lang="pt">Desativado</rdfs:label>
        <rdfs:label xml:lang="en">Disabled</rdfs:label>
        <owl:versionInfo xml:lang="pt">1</owl:versionInfo>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extEmergencyCriticality -->

    <owl:Class rdf:about="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extEmergencyCriticality">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extCriticalityValuePartition"/>
        <terms:created>14/Nov/2019</terms:created>
        <terms:creator>Katia Aleid</terms:creator>
        <rdfs:label xml:lang="en">Emergency</rdfs:label>
        <rdfs:label xml:lang="pt">Emergência</rdfs:label>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:versionInfo>
        <skos:definition xml:lang="en">Criticality value that the related event impacts to safety/breach of critical systems</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extEnabledStatus -->

    <owl:Class rdf:about="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extEnabledStatus">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extStatusValuePartition"/>
        <terms:created>14/Nov/2019</terms:created>
        <terms:creator>Katia Aleid</terms:creator>
        <rdfs:label xml:lang="pt">Ativado</rdfs:label>
        <rdfs:label xml:lang="en">Enabled</rdfs:label>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:versionInfo>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extEscalationCriticality -->

    <owl:Class rdf:about="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extEscalationCriticality">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extCriticalityValuePartition"/>
        <terms:created>14/Nov/2019</terms:created>
        <terms:creator>Katia Aleid</terms:creator>
        <rdfs:label xml:lang="en">Escalation</rdfs:label>
        <rdfs:label xml:lang="pt">Escalação</rdfs:label>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:versionInfo>
        <skos:definition xml:lang="en">Criticality value that the related event requires the application of corrective measures</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extEvent -->

    <owl:Class rdf:about="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extEvent">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extChanges"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extAsset"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extTriggers"/>
                <owl:someValuesFrom rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extAlert"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extHasCriticality"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extCriticalityValuePartition"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/2006/time#hasBeginning"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2006/time#Instant"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.w3.org/2006/time#hasEnd"/>
                <owl:qualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:qualifiedCardinality>
                <owl:onClass rdf:resource="http://www.w3.org/2006/time#Instant"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <terms:created>14/Nov/2019</terms:created>
        <terms:creator>Katia Aleid</terms:creator>
        <terms:modified>30/Nov/2019</terms:modified>
        <rdfs:label xml:lang="en">Event</rdfs:label>
        <rdfs:label xml:lang="pt">Evento</rdfs:label>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">2</owl:versionInfo>
        <skos:definition xml:lang="en">Discrete change of state or status of a system/device</skos:definition>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extExtremeSeverity -->

    <owl:Class rdf:about="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extExtremeSeverity">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extSeverityValuePartition"/>
        <terms:created>14/Nov/2019</terms:created>
        <terms:creator>Katia Aleid</terms:creator>
        <rdfs:label xml:lang="en">Extreme</rdfs:label>
        <rdfs:label xml:lang="pt">Extremo</rdfs:label>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:versionInfo>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extHighSeverity -->

    <owl:Class rdf:about="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extHighSeverity">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extSeverityValuePartition"/>
        <terms:created>14/Nov/2019</terms:created>
        <terms:creator>Katia Aleid</terms:creator>
        <rdfs:label xml:lang="pt">Alto</rdfs:label>
        <rdfs:label xml:lang="en">High</rdfs:label>
        <owl:versionInfo rdf:datatype="http://www.w3.org/2001/XMLSchema#integer">1</owl:versionInfo>
    </owl:Class>
    


    <!-- http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extIncident -->

    <owl:Class rdf:about="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extIncident">
        <rdfs:subClassOf rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extThing"/>
        <rdfs:subClassOf>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extConsistsOf"/>
                <owl:minQualifiedCardinality rdf:datatype="http://www.w3.org/2001/XMLSchema#nonNegativeInteger">1</owl:minQualifiedCardinality>
                <owl:onClass rdf:resource="http://www.semanticweb.org/katiaaleid(1181473)/ontologies/2019/10/ontology-extension#extEvent"/>
            </owl:Restriction>
        </rdfs:subClassOf>
        <rdfs:subClassOf>
